{
  "compilerOptions": {
    "strict": true,
    "target": "es2020",
    "lib": [
      "es2020"
    ],
    "module": "ES6",
    "moduleResolution": "node",
    "outDir": "test",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "declaration": true,
  },
  "include": [
    "src/**/*"
   ]
}