    "compilerOptions": {
      "strict": true,
      "target": "es2020",
      "lib": ["es2020"],
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "lib",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "resolveJsonModule": true,
      "baseUrl": ".",
    }
}